{
  "name": "react-app",
  "private": true,
  "version": "0.0.0",
  "homepage": "https://{username}.github.io/{repo-name}/",
  "type": "module",
  "browserslist": "> 0.25%, not dead",
  "scripts": {
    "builddev": "npm run build && npm run dev",
    "dev": "node server.cjs",
    "build": "npm run copy-assets && npm run bundle",
    "typescript-compile": "rimraf temp && tsc",
    "bundle": "rollup -c -w",
    "copy-assets": "powershell -Command Copy-Item -Path src/assets -Destination ./dist -recurse -force",
    "lint": "eslint --cache --ext .ts src/**",
    "format": "prettier --write --cache --parser typescript \"src/**/*.ts\"",
    "prepublishOnly": "npm run build",
    "deploy": "gh-pages -d dist",

    
    "rollup-with-typescript": "rollup -c"
  },
  "dependencies": {
    "core-js": "^3.36.0",
    "help": "^3.0.2",
    "localforage": "^1.10.0",
    "match-sorter": "^6.3.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-image-gallery": "^1.3.0",
    "react-router-dom": "^6.11.1",
    "rimraf": "^5.0.5",
    "sort-by": "^0.0.2",
    "tslib": "^2.6.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.23.9",
    "@babel/core": "^7.24.0",
    "@babel/plugin-transform-react-jsx": "^7.23.4",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.24.0",
    "@babel/preset-react": "^7.23.3",
    "@babel/preset-typescript": "^7.23.3",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-image": "^3.0.3",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.6",
    "@rollup/plugin-url": "^8.0.2",
    "@types/node": "^20.11.5",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@types/react-image-gallery": "^1.2.4",
    "esbuild": "0.20.0",
    "gh-pages": "^6.1.1",
    "npm-run-all": "^4.1.5",
    "rollup": "^4.10.0",
    "rollup-plugin-css-only": "^4.5.2",
    "rollup-plugin-css-porter": "^1.0.2",
    "rollup-plugin-glob-import": "^0.5.0",
    "rollup-plugin-polyfill-node": "^0.13.0",
    "rollup-plugin-styles": "^4.0.0",
    "rollup-plugin-svg": "^2.0.0",
    "typescript": "^4.9.5"
  }
}
